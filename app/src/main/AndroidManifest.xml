<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ahmadabuhasan.skripsi">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_me"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_me_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Skripsi">
        <activity android:name=".data.EditProductActivity"></activity>
        <activity android:name=".data.AddProductActivity"></activity>
        <activity android:name=".data.ProductActivity" />
        <activity
            android:name=".DashboardActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SplashActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="landscape"
            android:theme="@style/SplashScreen" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>